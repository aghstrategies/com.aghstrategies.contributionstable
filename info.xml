<?xml version="1.0"?>
<extension key="com.aghstrategies.contributionstable" type="module">
  <file>contributionstable</file>
  <name>Contributions table token</name>
  <description>Uses token to build contribution tables for individual contact records</description>
  <license>agpl</license>
  <maintainer>
    <author>AGH Strategies</author>
    <email>info@aghstrategies.com</email>
  </maintainer>
  <urls>
    <url desc="Documentation">https://github.com/aghstrategies/com.aghstrategies.contributionstable/blob/master/README.md</url>
  </urls>
  <releaseDate>2025-01-13</releaseDate>
  <version>1.2</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <civix>
    <namespace>CRM/Contributionstable</namespace>
    <format>24.09.1</format>
  </civix>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <mixins>
    <mixin>entity-types-php@2.0.0</mixin>
  </mixins>
</extension>
